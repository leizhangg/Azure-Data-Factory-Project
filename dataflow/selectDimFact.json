{
	"name": "selectDimFact",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "join_output_final",
						"type": "DatasetReference"
					},
					"name": "integrationTableSource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "output_date_Dim",
						"type": "DatasetReference"
					},
					"name": "sinkDateDim"
				},
				{
					"dataset": {
						"referenceName": "output_Purchase_Dim",
						"type": "DatasetReference"
					},
					"name": "sinkPurchaseDim"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "surrogateOrderKey"
				},
				{
					"name": "select3"
				},
				{
					"name": "surrogateKey1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PurchaseOrderLineID as short,",
				"          PurchaseOrderID as short,",
				"          StockItemID as short,",
				"          OrderedOuters as short,",
				"          Description as string,",
				"          ReceivedOuters as double,",
				"          PackageTypeID as short,",
				"          ExpectedUnitPricePerOuter as double,",
				"          LastReceiptDate as date,",
				"          IsOrderLineFinalized as boolean,",
				"          LastEditedBy as short,",
				"          LastEditedWhen as date,",
				"          ingest_time as date,",
				"          surrogateKeyPOL as short,",
				"          SupplierID as short,",
				"          OrderDate as date,",
				"          DeliveryMethodID as short,",
				"          ContactPersonID as short,",
				"          ExpectedDeliveryDate as date,",
				"          SupplierReference as string,",
				"          IsOrderFinalized as boolean,",
				"          Comments as string,",
				"          InternalComments as string,",
				"          surrogateKeyPO as integer,",
				"          SupplierName as string,",
				"          SupplierCategoryID as integer,",
				"          PrimaryContactPersonID as integer,",
				"          AlternateContactPersonID as integer,",
				"          DeliveryCityID as integer,",
				"          PostalCityID as integer,",
				"          BankAccountName as string,",
				"          BankAccountBranch as string,",
				"          BankAccountCode as integer,",
				"          BankAccountNumber as string,",
				"          BankInternationalCode as short,",
				"          PaymentDays as integer,",
				"          PhoneNumber as string,",
				"          FaxNumber as string,",
				"          WebsiteURL as string,",
				"          DeliveryAddressLine1 as string,",
				"          DeliveryAddressLine2 as string,",
				"          DeliveryPostalCode as integer,",
				"          DeliveryLocation as string,",
				"          PostalAddressLine1 as string,",
				"          PostalAddressLine2 as string,",
				"          PostalPostalCode as integer,",
				"          ValidFrom as timestamp,",
				"          ValidTo as timestamp,",
				"          surrogate_key_supplier as short,",
				"          SupplierCategoryName as string,",
				"          surrogate_key_SC as integer,",
				"          SupplierTransactionID as integer,",
				"          TransactionTypeID as short,",
				"          PaymentMethodID as integer,",
				"          SupplierInvoiceNumber as integer,",
				"          TransactionDate as date,",
				"          AmountExcludingTax as double,",
				"          TaxAmount as double,",
				"          TransactionAmount as double,",
				"          OutstandingBalance as double,",
				"          FinalizationDate as date,",
				"          IsFinalized as boolean,",
				"          surrogate_key_ST as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> integrationTableSource",
				"integrationTableSource derive(Day = dayOfMonth(OrderDate),",
				"          {Day Number} = dayOfMonth(OrderDate),",
				"          {Calendar Month Number} = month(OrderDate),",
				"          {Calendar Year} = year(OrderDate),",
				"          {ISO Week Number} = weekOfYear(OrderDate),",
				"          Month = month(OrderDate),",
				"          column1 = ERROR_FUNCTION('')) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          Date = OrderDate,",
				"          Day,",
				"          {Calendar Month Number},",
				"          {Calendar Year},",
				"          {Day Number},",
				"          {ISO Week Number}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"integrationTableSource select(mapColumn(",
				"          PurchaseOrderID,",
				"          Description,",
				"          PackageTypeID,",
				"          IsOrderFinalized,",
				"          IsFinalized",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 keyGenerate(output({Order Key} as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateOrderKey",
				"integrationTableSource select(mapColumn(",
				"          {Supplier ID} = SupplierID,",
				"          {City ID} = DeliveryCityID,",
				"          {Payment Method ID} = PaymentMethodID,",
				"          Date = OrderDate,",
				"          {Transaction Type ID} = SupplierTransactionID,",
				"          OrderedOuters,",
				"          ReceivedOuters,",
				"          ExpectedUnitPricePerOuter,",
				"          TransactionAmount,",
				"          AmountExcludingTax,",
				"          TaxAmount,",
				"          OutstandingBalance,",
				"          IsOrderLineFinalized,",
				"          IsOrderFinalized,",
				"          IsFinalized,",
				"          surrogateKeyPOL,",
				"          surrogateKeyPO,",
				"          surrogate_key_supplier,",
				"          surrogate_key_SC",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3",
				"select1 keyGenerate(output({Date Key} as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['Dimension_Date.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinkDateDim",
				"surrogateOrderKey sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string",
				"     ),",
				"     partitionFileNames:['Dimension_Purchasing.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sinkPurchaseDim"
			]
		}
	}
}